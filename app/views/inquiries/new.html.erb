<%= form_with(model: Inquiry.new, local: true) do |form| %>
  <%= form.label :name %><br>
  <%= form.text_field :name %><br>
  
  <%= form.label :email %><br>
  <%= form.text_field :email %><br>

  <%= form.label :content %><br>
  <%= form.text_field :content %><br>
  
  <%= form.submit %>
<% end %>